{
  "name": "itpc-manager",
  "version": "1.0.0",
  "description": "ITPC Manager",
  "author": "nanocloudx <nanoclodx@gmail.com>",
  "private": true,
  "scripts": {
    "start": "webpack --mode production && yarn serve",
    "watch": "webpack --mode development --watch & yarn serve",
    "serve": "node --experimental-modules ./server/bin/www",
    "lint": "eslint --fix './**/*.mjs'",
    "test": "jest",
    "db:migrate": "sequelize db:migrate"
  },
  "dependencies": {
    "compression": "^1.7.2",
    "connect-redis": "^3.3.3",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "helmet": "^3.12.1",
    "hjs": "^0.0.6",
    "http-errors": "^1.6.3",
    "ioredis": "^3.2.2",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-github": "^1.1.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^4.0.0",
    "uuid": "^3.2.1",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.4",
    "babel-jest": "^23.0.1",
    "css-loader": "^0.28.11",
    "eslint": "^5.0.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.5.0",
    "jest": "^23.1.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.3",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "mjs",
      "json",
      "vue"
    ],
    "testMatch": [
      "**/__tests__/**/*.mjs"
    ],
    "transform": {
      "^.+\\.mjs$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "verbose": true
  }
}
